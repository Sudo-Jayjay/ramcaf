<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ramcaf">

    <uses-permission
        android:name="android.permission.INTERNET"
        android:usesCleartextTraffic="true" />
    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ramcaf_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RAMCaf">
        <activity android:name=".UserSeatStatus">
        </activity>
        <activity android:name=".UserScanSeat">
        </activity>
        <activity android:name=".StaffSetShortStatus">
        </activity>
        <activity android:name=".StaffSetLongStatus">
        </activity>
        <activity android:name=".StaffChooseSetStatus">
        </activity>
        <activity android:name=".CheckStatuses" >
        </activity>
        <activity android:name=".CheckShortStatuses">
        </activity>
        <activity android:name=".CheckLongStatuses">
        </activity>
        <activity android:name=".StaffDisinfect">
        </activity>
        <activity android:name=".StaffHome">
        </activity>
        <activity android:name=".AdminHome">
        </activity>
        <activity android:name=".StaffLogin" />
        <activity android:name=".AdminLogin" />
        <activity android:name=".TermsAndConditions" />
        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Signup" />
        <activity android:name=".MainActivity" >
        </activity>
    </application>
</manifest>